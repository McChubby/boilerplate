{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build-babel && npm run build",
    "build": "webpack --watch --progress --hide-modules",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "clean": "rimraf dist node_modules",
    "start": "node ./dist/server.js",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/McChubby/boilerplate.git"
  },
  "keywords": [
    "Node",
    "React"
  ],
  "author": "Ruan kok",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/McChubby/boilerplate/issues"
  },
  "homepage": "https://github.com/McChubby/boilerplate#readme",
  "jest": {
    "setupTestFrameworkScriptFile": "./src/enzyme.setup.js"
  },
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "webpack": "^4.19.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.5.1",
    "source-map-loader": "^0.2.4",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
